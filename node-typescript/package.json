{
  "name": "bootstrap-typescript",
  "version": "0.0.1",
  "description": "",
  "author": "Juan Carlos Serrano",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "bin": {
    "greeting": "bin/main"
  },
  "main": "src/index",
  "scripts": {
    "lint": "eslint src test bin",
    "lint:fix": "npm run lint -- --fix",
    "build": "tsc -p .",
    "build:live": "npm run build -- -w",
    "test": "jest",
    "test:coverage": "npm t -- --coverage",
    "tdd": "jest --watchAll",
    "tsn": "ts-node"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^12.20.52",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.0",
    "jest-extended": "^2.0.0",
    "jest-junit": "^13.2.0",
    "jest-summary-reporter": "0.0.2",
    "lint-staged": "^12.4.1",
    "husky": "^4.3.8",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:coverage",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.ts": "npm run lint:fix"
  }
}
